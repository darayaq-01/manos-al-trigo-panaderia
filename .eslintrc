{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "preact", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
      "@typescript-eslint/rule-name": "error"
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {},
  "settings": {
    "import/resolver": {
      "typescript": {}
    }
  }
}
